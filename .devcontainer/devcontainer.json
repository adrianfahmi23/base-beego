{
  "name": "Go Dev",
  "dockerComposeFile": [
    "../docker-compose.yml"
  ],
  "workspaceFolder": "/go/src/app",
  "service": "app",
  "customizations": {
    "vscode": {
      "settings": {
        "go.toolsManagement.autoUpdate": true,
        "go.useLanguageServer": true
      },
      "extensions": ["golang.go", "esbenp.prettier-vscode"]
    }
  },
  "remoteUser": "root"
}
